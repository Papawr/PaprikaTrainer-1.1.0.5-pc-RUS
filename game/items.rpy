label inspInjectorSmall:
    if task4Stage <= 6:
        "A small syringe with nanobots."
    elif True:
        "A small syringe with nanobots. You can inject your spies with these for more skills and abilities, but raises nanobots levels, making them harder to control."
    jump items
label useInjectorSmall:
    if task4Stage <= 6:
        y "We don't know what these do yet. Best leave them alone until we found out."
        jump items
    if injectorSmall >= 1:
        scene bgInjector
        show screen injectUI
        call screen injectButton
    elif True:
        hide screen injectUI
        "Out of nanobot injectors."
        jump items

label injectButton:
    $ injectorSmall -= 1
    $ nanoLevelSam += 25
    $ nanoLevelClover += 25
    $ nanoLevelAlex += 25
    if nanoLevelSam >= 100:
        $ nanoLevelSam = 100
    if nanoLevelClover >= 100:
        $ nanoLevelClover = 100
    if nanoLevelAlex >= 100:
        $ nanoLevelAlex = 100
    play sound "audio/sfx/shoot1.mp3"
    pause 0.3
    show screen skillBar
    while injectorCapacity > 0:
        $ injectorCapacity -= 10
        $ skillPoints += 10
        if skillPoints >= 100 and skillRank == 1:
            hide screen skillBar with d3
            $ skillRank = 2
            $ skillPoints = 0
            $ nanobotLevel = 200
            "You unlocked a new skill!"
            play sound "audio/sfx/itemGot.mp3"
            "{b}Power Spy{/b} - Increases takedown chance by 15%%."
            $ skillTakedown = 3
        elif skillPoints >= 200 and skillRank == 2:
            hide screen skillBar with d3
            $ skillRank = 3
            $ skillPoints = 0
            $ nanobotLevel = 300
            "You unlocked a new skill!"
            play sound "audio/sfx/itemGot.mp3"
            "{b}Wizkid{/b} - Hacking boost now increases your hacking chance by 25%%."
            $ skillHack = 3
        elif skillPoints >= 200 and skillRank == 3:
            hide screen skillBar with d3
            $ skillRank = 4
            $ skillPoints = 0
            $ nanobotLevel = 200
            "You unlocked a new skill!"
            play sound "audio/sfx/itemGot.mp3"
            "{b}Second Chance{/b} - Instead of getting knocked out, your spies have a chance to block enemy attacks."
            $ skillSecondChance = 1
        elif skillPoints >= 200 and skillRank == 4:
            $ skillRank = 5
            $ skillPoints = 0
            $ nanobotLevel = 200
            "You unlocked a new skill!"
            play sound "audio/sfx/itemGot.mp3"
            "{b}Happiness Juice{/b} - Spy morale improves quicker at night."
        elif skillPoints >= 200 and skillRank == 5:
            $ skillRank = 6
            $ skillPoints = 0
            $ nanobotLevel = 200
            "You unlocked a new skill!"
            play sound "audio/sfx/itemGot.mp3"
            "{b}Flexible{/b} - Spies bring in more money while stripping."
        elif skillPoints >= 200 and skillRank == 6:
            $ skillRank = 7
            $ skillPoints = 0
            $ nanobotLevel = 200
            "You unlocked a new skill!"
            play sound "audio/sfx/itemGot.mp3"
            "{b}Bad Intend{/b} - Notoriety doesn't drop below 25."
        pause 0.1
    $ injectorCapacity = 100
    hide screen skillBar
    jump useInjectorSmall
jump items

label inspMedkit:
    "A medkit that will restore a bit of health to wounded spies."
    jump items
label useMedkit:
    menu:
        "Sam [samHealth]/3" if True:
            if samHealth >= 3:
                "Sam is already fully healed."
                jump useMedkit
            $ medkit -= 1
            $ samHealth += 1
            if samHealth >= 3:
                $ samHealth = 3
            play sound "audio/sfx/bandage.mp3"
            if medkit >= 1:
                jump useMedkit
        "Clover [cloverHealth]/3" if True:
            if cloverHealth >= 3:
                "Clover is already fully healed."
                jump useMedkit
            $ medkit -= 1
            $ cloverHealth += 1
            if cloverHealth >= 3:
                $ cloverHealth = 3
            play sound "audio/sfx/bandage.mp3"
            if medkit >= 1:
                jump useMedkit
        "Alex [alexHealth]/3" if True:
            if alexHealth >= 3:
                "Alex is already fully healed."
                jump useMedkit
            $ medkit -= 1
            $ alexHealth += 1
            if alexHealth >= 3:
                $ alexHealth = 3
            play sound "audio/sfx/bandage.mp3"
            if medkit >= 1:
                jump useMedkit
        "Done" if True:
            pass
    jump items

