    if candyInteract == 1:
        $ hallCandy -= 1
        show scene_darkening with d3
        if spyGreenActive and greenDayJob == 0:
            $ samMood += 5
            show green g1 at le:
            with d3
            s "A quick candy break? Sure."
        if spyRedActive and redDayJob == 0:
            $ samMood += 5
            show red r1 at ce with d3
            c "Just one then.... maybe one more... gimme that!"
        if spyYellowActive and yellowDayJob == 0:
            $ samMood += 5
            show yellow y1 at ri with d3
            a "Yay candy!"
        if greenDayJob >= 1 and redDayJob >= 1 and yellowDayJob >= 1:
            y "Free candy for all!"
            "..................................."
            y "Oh right, everybody's out..."
            y "Well more for me...!"
            hide scene_darkening with d3
            "You ate all of the candy.{w} Remember to brush your teeth!"
        play sound "audio/sfx/jobReward.mp3"
        "The everyone's mood has increased slightly."
        hide green
        hide red
        hide yellow
        hide scene_darkening
        with d3
    if candyInteract == 2:
        show scene_fighting
        show candyBig:
            xalign 0.5 yalign 0.5
        with d3
        "A pile of diverse Halloween cand-...{w} LICORICE?! {w}What sick monster gives licorice?!"
        hide scene_fighting
        hide candyBig
        with d3
    jump items


